rule RO_EG_SU
	when
		Item RO_EG_SU received command
	then
    var String message="!w!RO_EG_SU!0$\n!w!RO_EG_SU_ON!0$\n!w!RO_EG_SU_UD!0$\n"
	if (receivedCommand==STOP) {postUpdate(RO_EG_SU, 50) }
	if (receivedCommand==UP) {message="!w!RO_EG_SU!1$\n"}
	if (receivedCommand==DOWN) {message="!w!RO_EG_SU!-1$\n"}
    sendCommand(RS485_Gateway_out, message)
    println("rs485 out " + message)
end

